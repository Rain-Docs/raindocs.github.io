{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "./src",
          "files": [
            "**/*.dll"
          ]
        }
      ],
      "dest": "api",
      "references": [
        {
          "file": "./libs/Assembly-CSharp-firstpass.dll"
        },
        {
          "file": "./libs/com.rlabrecque.steamworks.net.dll"
        },
        {
          "file": "./libs/GoKit.dll"
        },
        {
          "file": "./libs/HOOKS-Assembly-CSharp.dll"
        },
        {
          "file": "./libs/MonoMod.RuntimeDetour.dll"
        },
        {
          "file": "./libs/mscorlib.dll"
        },
        {
          "file": "./libs/Rewired.Runtime.dll"
        },
        {
          "file": "./libs/Rewired_Core.dll"
        },
        {
          "file": "./libs/System.dll"
        },
        {
          "file": "./libs/System.Core.dll"
        },
        {
          "file": "./libs/Unity.Burst.dll"
        },
        {
          "file": "./libs/Unity.Mathematics.dll"
        },
        {
          "file": "./libs/UnityEngine.AssetBundleModule.dll"
        },
        {
          "file": "./libs/UnityEngine.AudioModule.dll"
        },
        {
          "file": "./libs/UnityEngine.CoreModule.dll"
        },
        {
          "file": "./libs/UnityEngine.GameCenterModule.dll"
        },
        {
          "file": "./libs/UnityEngine.ImageConversionModule.dll"
        },
        {
          "file": "./libs/UnityEngine.IMGUIModule.dll"
        },
        {
          "file": "./libs/UnityEngine.InputLegacyModule.dll"
        },
        {
          "file": "./libs/UnityEngine.ParticleSystemModule.dll"
        },
        {
          "file": "./libs/UnityEngine.PhysicsModule.dll"
        },
        {
          "file": "./libs/UnityEngine.ScreenCaptureModule.dll"
        },
        {
          "file": "./libs/UnityEngine.UI.dll"
        },
        {
          "file": "./libs/UnityEngine.UIModule.dll"
        },
        {
          "file": "./libs/UnityEngine.UnityWebRequestAudioModule.dll"
        },
        {
          "file": "./libs/UnityEngine.UnityWebRequestModule.dll"
        },
        {
          "file": "./libs/UnityEngine.UnityWebRequestWWWModule.dll"
        }
      ],
      "disableDefaultFilter": true,
      "globalNamespaceId": "Global"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "RainDocs",
      "_appTitle": "RainDocs",
      "_enableSearch": true,
      "pdf": true
    }
  }
}